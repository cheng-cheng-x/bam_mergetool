{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "/data/haocheng/project/analyze/text/target/debug/text", // 替换为你的程序路径
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "cargo build",
            "miDebuggerPath": "/data/haocheng/miniconda3/envs/rust/bin/gdb" // 根据你的环境设置 gdb 路径
        }
    ]
}
